<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.a.compose">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ComposeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compose">
        <activity
            android:name=".MainActivity2"
            android:exported="true" />
        <activity
            android:name=".SampleListActivity"
            android:exported="true"
            android:label="@string/title_activity_sample_list"
            android:theme="@style/Theme.Compose.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AndroidNavGraph"
            android:exported="true"
            android:label="@string/title_activity_android_nav_graph"
            android:theme="@style/Theme.Compose.NoActionBar"></activity>
        <activity
            android:name=".NavigationV1"
            android:exported="true"
            android:label="@string/title_activity_navigation_v1"
            android:theme="@style/Theme.Compose.NoActionBar" />
        <activity
            android:name=".SampleActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Compose.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Compose.NoActionBar" />
    </application>

</manifest>